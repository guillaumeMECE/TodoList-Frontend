{
  "name": "todolist-frontend",
  "version": "1.0.0",
  "description": "NODE.JS & VUE.JS APP",
  "main": ".eslintrc.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-back": "cross-env NODE_ENV=development nodemon src/backend/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec \"npm run lintfix && node frontend/server/index.js\"",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate"
  },
